{
    "preset": "laravel",
    "rules": {
        "class_attributes_separation": {
            "elements": {
                "const": "none",
                "method": "one",
                "property": "none",
                "trait_import": "none",
                "case": "none"
            }
        },
        "no_short_bool_cast": false,
        "not_operator_with_space": false,
        "not_operator_with_successor_space": false,
        "concat_space": {
            "spacing": "one"
        }
    }
}